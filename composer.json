{
  "name": "koriym/dii",
  "description": "Dependency Injection Plugin for Yii 1",
  "require": {
      "php": ">=7.1.0",
      "yiisoft/yii": "1.1.*",
      "ray/di": "^2.0"
  },
  "autoload": {
      "psr-4": {
          "Koriym\\Dii\\": ["src/"]
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ray\\Dyii\\Module\\": "demo/src/Module"
    },
    "classmap": [
          "vendor/yiisoft/yii/framework/base/",
          "vendor/yiisoft/yii/framework/web/",
          "vendor/yiisoft/yii/framework/console/"
    ]
  },
  "license": "MIT",
  "scripts" :{
      "tests": ["psalm --show-info=false", "@cs"],
      "cs": ["php-cs-fixer fix -v --dry-run", "phpcs --standard=./phpcs.xml src"],
      "cs-fix": ["php-cs-fixer fix -v", "phpcbf src"]
  },
  "require-dev": {
      "vimeo/psalm": "^3.4"
  }
}
