{
  "name": "koriym/dii",
  "description": "Dependency Injection Plugin for Yii 1",
  "require": {
      "php": ">=7.1.0",
      "yiisoft/yii": "1.1.*",
      "ray/di": "^2.0"
  },
  "autoload": {
      "psr-4": {
          "Koriym\\Dii\\": ["src/"]
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ray\\Dyii\\Module\\": "demo/src/Module"
    },
    "classmap": [
          "vendor/yiisoft/yii/framework/base/",
          "vendor/yiisoft/yii/framework/web/"
    ]
  },
  "license": "MIT",
  "scripts" :{
      "test": ["phpunit"],
      "tests": ["psalm --show-info=false", "@cs"],
      "cs": ["php-cs-fixer fix -v --dry-run"],
      "cs-fix": ["php-cs-fixer fix -v"]
  },
  "require-dev": {
      "vimeo/psalm": "^4.2",
      "phpunit/phpunit": "^7.5",
      "friendsofphp/php-cs-fixer": "^2.16"
  }
}
